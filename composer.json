{
    "name": "breathingboard/flarum-ext-audio",
    "description": "Adds an [audio] and a [sound] BBCode tag to Flarum.",
    "type": "flarum-extension",
    "keywords": ["formatting", "audio", "mp3", "ogg", "flac", "soundmanager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Sobkanyuk",
            "email": "github@chapay.com"
        },
        {
            "name": "Billy Wilcosky",
            "email": "admin@breathingboard.com",
            "homepage": "https://breathingboard.com"
        }
    ],
    "support": {
        "issues": "https://github.com/breathingboard/flarum-ext-audio/issues",
        "source": "https://github.com/breathingboard/flarum-ext-audio"
    },
    "require": {
        "flarum/core": "*"
    },
    "autoload": {
        "psr-4": {
            "Breathingboard\\Audio\\": "src/",
            "Breathingboard\\Audio\\": "assets/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Audio",
            "icon": {
                "name": "play",
                "backgroundColor": "#333",
                "color": "#fff"
            }
        }
    }
}
